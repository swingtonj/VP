<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Analytics Value Predictor</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark fixed-top ">
            <div class="container">
                <a class="navbar-brand" href="#">Analytics Value Predictor</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbars" aria-controls="navbars" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbars">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a id="aRegister" class="nav-link btn btn-lg btn-primary" href='@Url.Action("index","home")'>Register</a>
                        </li>
                        <li class="nav-item active">
                            <a id="aSpecify" class="nav-link btn btn-lg btn-primary" href="@Url.Action("index","specify")">Specify</a>
                        </li>
                        <li class="nav-item active">
                            <a  id="aResult" class="nav-link btn btn-lg btn-primary" href="@Url.Action("result","specify")">Results</a>
                        </li>
                        <li class="nav-item active">
                            <a id="aSensitivity" class="nav-link btn btn-lg btn-primary" href="@Url.Action("index","sensitivity")">Sensitivity</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="body-content">
        @RenderBody()
        <footer class="footer" style="padding:20px 10px 10px 10px;margin-top:30px;">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <a href="http://www.cabotpartners.com/" target="_blank">About us</a> | <a href="http://www.ibm.com/privacy/us/en/?lnk=flg-priv-usen?lnk=flg" target="_blank">Privacy</a> | <a href="http://www.ibm.com/legal/us/en/?lnk=flg-tous-usen?lnk=flg" target="_blank">Terms & Condition</a> | Contact : <a href="mailto:info@cabotpartners.com">info@cabotpartners.com</a> | Copyright® @DateTime.Now.Year - Cabot Partners Group. Inc.
                    </div>

                </div>
            </div>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
@Scripts.Render("~/bundles/common")
</html>
